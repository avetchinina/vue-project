{
  "name": "vue-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/font": "~3.7.95",
    "axios": "~0.19.0",
    "bootstrap": "~4.3.1",
    "core-js": "~2.6.5",
    "vue": "~2.6.10",
    "vue-router": "~3.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~3.9.0",
    "@vue/cli-plugin-eslint": "~3.9.0",
    "@vue/cli-service": "~3.9.0",
    "@vue/eslint-config-prettier": "~4.0.1",
    "babel-eslint": "~10.0.1",
    "eslint": "~5.16.0",
    "eslint-plugin-vue": "~5.2",
    "json-server": "~0.15.0",
    "less": "~3.9.0",
    "less-loader": "~4.1.0",
    "lint-staged": "~8.2.1",
    "vue-template-compiler": "~2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
